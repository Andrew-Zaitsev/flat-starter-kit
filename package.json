{
  "name": "fsd-flat-starter-kit",
  "version": "1.0.0",
  "description": "Starter Kit for simple HTML+CSS pages (flat-pages) built by FSD team",
  "main": "server.js",
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "dev": "better-npm-run start-dev",
    "build": "better-npm-run build-prod",
    "test": "test"
  },
  "betterScripts": {
    "build-prod": {
      "command": "./node_modules/.bin/webpack",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstack-development/flat-starter-kit.git"
  },
  "keywords": [
    "Starter Kit",
    "Flat Pages",
    "Pug",
    "Stylus",
    "Webpack",
    "BEM"
  ],
  "author": "Fullstack Development Llc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullstack-development/flat-starter-kit/issues"
  },
  "homepage": "https://github.com/fullstack-development/flat-starter-kit#readme",
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "better-npm-run": "0.0.13",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jquery": "^3.1.1",
    "pug": "^2.0.0-beta6",
    "pug-html-loader": "^1.0.9",
    "pug-loader": "^2.3.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-config": "^6.2.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.1"
  }
}
